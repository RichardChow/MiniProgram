.container {
    min-height: 100vh;
    background: #fff;
  }
  
  .content {
    width: 100%;
  }
  
  .logout-btn {
    color: #ff4d4f;
    font-size: 28rpx;
  }
  
  .title {
    font-size: 36rpx;
    font-weight: 500;
    text-align: center;
    margin: 20rpx 0 30rpx;
  }
  
  .page-container {
    padding: 30rpx;
    background: #f5f6fa;
  }
  
  .filter-section {
    background: #fff;
    padding: 30rpx;
    margin-bottom: 20rpx;
  }
  
  .date-picker {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    background: #f5f7fa;
    margin-bottom: 20rpx;
  }
  
  .status-filter {
    display: flex;
    gap: 20rpx;
  }
  
  .filter-item {
    flex: 1;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
    background: #f5f6fa;
    border-radius: 32rpx;
    font-size: 26rpx;
    color: #666;
    transition: all 0.3s;
  }
  
  .filter-item.active {
    background: #1890ff;
    color: #fff;
    box-shadow: 0 2rpx 8rpx rgba(24, 144, 255, 0.3);
  }
  
  .reservation-card {
    background: #fff;
    border-radius: 16rpx;
    padding: 30rpx;
    margin-bottom: 30rpx;
    border: 1rpx solid #eee;
  }
  
  .card-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 20rpx;
    padding-bottom: 20rpx;
    border-bottom: 2rpx solid #f5f5f5;
  }
  
  .time {
    font-size: 32rpx;
    font-weight: 500;
  }
  
  .status {
    font-size: 26rpx;
    padding: 4rpx 16rpx;
    border-radius: 4rpx;
  }
  
  .status.upcoming {
    color: #1890ff;
    background: #e6f7ff;
  }
  
  .status.completed {
    color: #52c41a;
    background: #f6ffed;
  }
  
  .status.cancelled {
    color: #999;
    background: #f5f5f5;
  }
  
  .info-row {
    display: flex;
    margin-bottom: 16rpx;
  }
  
  .info-row .label {
    width: 160rpx;
    color: #666;
  }
  
  .time-slots {
    display: flex;
    flex-wrap: wrap;
    gap: 16rpx;
    margin-top: 8rpx;
    margin-bottom: 8rpx;
    justify-content: center;
    align-items: center;
  }
  
  .time-tag {
    font-size: 26rpx;
    color: #1890ff;
    background: #e6f7ff;
    padding: 4rpx 16rpx;
    border-radius: 4rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    min-width: 120rpx;
  }
  
  .card-footer {
    margin-top: 30rpx;
    padding-top: 20rpx;
    border-top: 2rpx solid #f5f5f5;
  }
  
  .cancel-btn {
    width: 100%;
    height: 80rpx;
    line-height: 80rpx;
    text-align: center;
    border-radius: 40rpx;
    color: #ff4d4f;
    border: 2rpx solid #ff4d4f;
    background: #fff;
    font-size: 28rpx;
  }
  
  .nav-bar {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    height: 88rpx;
    background: #FFFFFF;
    display: flex;
    align-items: center;
    padding: 0 24rpx;
    box-shadow: 0 2rpx 8rpx rgba(0, 0, 0, 0.05);
    z-index: 100;
  }
  
  .nav-title {
    flex: 1;
    text-align: center;
    font-size: 34rpx;
    font-weight: 500;
    color: #333333;
  }
  
  .logout-icon {
    width: 32rpx;
    height: 32rpx;
    margin-right: 8rpx;
  }
  
  .login-modal {
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.5);
    display: flex;
    justify-content: center;
    align-items: center;
    z-index: 999;
  }
  
  .login-box {
    width: 80%;
    background: #fff;
    border-radius: 16rpx;
    padding: 40rpx;
  }
  
  .login-title {
    font-size: 32rpx;
    font-weight: 500;
    text-align: center;
    margin-bottom: 40rpx;
  }
  
  .password-input-container {
    position: relative;
    margin-bottom: 40rpx;
  }
  
  .login-input {
    width: 100%;
    height: 88rpx;
    background: #f5f6fa;
    border-radius: 8rpx;
    padding: 0 30rpx 0 30rpx;
    box-sizing: border-box;
  }
  
  .login-btn {
    width: 100%;
    height: 88rpx;
    line-height: 88rpx;
    background: #1890ff;
    color: #fff;
    border-radius: 44rpx;
    font-size: 32rpx;
    text-align: center;
  }
  
  .password-toggle {
    position: absolute;
    right: 20rpx;
    top: 50%;
    transform: translateY(-50%);
    padding: 10rpx;
  }
  
  .eye-icon {
    width: 40rpx;
    height: 40rpx;
  }
  
  .calendar-icon {
    width: 36rpx;
    height: 36rpx;
    margin-right: 16rpx;
    opacity: 0.6;
  }
  
  .stats-section {
    display: flex;
    justify-content: space-around;
    padding: 30rpx;
    background: #fff;
    margin-bottom: 20rpx;
  }
  
  .stat-item {
    flex: 1;
    text-align: center;
    position: relative;
    display: flex;
    justify-content: center;
    align-items: center;
  }
  
  .stat-content {
    display: flex;
    flex-direction: column;
    align-items: center;
  }
  
  .stat-number {
    display: block;
    font-size: 36rpx;
    font-weight: 500;
    color: #1890ff;
    margin-bottom: 8rpx;
  }
  
  .stat-label {
    font-size: 24rpx;
    color: #666;
    text-align: center;
  }
  
  .reservation-list {
    margin-top: 24rpx;
    height: calc(100vh - 88rpx - 280rpx);
  }

  .header {
    margin-bottom: 30rpx;
  }
  
  .logout-row {
    text-align: center;
  }
  
  .title-row {
    text-align: center;
    margin-bottom: 20rpx;
  }
  
  .page-title {
    font-size: 40rpx;
    font-weight: 600;
    color: #333;
  }
  
  .logout-btn {
    display: inline-block;
    padding: 12rpx 30rpx;
    background: #FFF0F0;
    border-radius: 30rpx;
  }
  
  .logout-text {
    color: #FF4D4F;
    font-size: 28rpx;
  }
  
  .stats-card {
    background: #FFFFFF;
    border-radius: 16rpx;
    padding: 40rpx 30rpx;
    display: flex;
    justify-content: space-between;
    margin-bottom: 30rpx;
  }
  
  .date-picker-card {
    background: #FFFFFF;
    border-radius: 16rpx;
    padding: 24rpx;
    margin-bottom: 30rpx;
  }
  
  .date-picker {
    display: flex;
    align-items: center;
    height: 80rpx;
    padding: 0 24rpx;
    background: #F5F7FA;
    border-radius: 8rpx;
  }
  
  .filter-card {
    background: #FFFFFF;
    border-radius: 16rpx;
    padding: 24rpx;
    margin-bottom: 30rpx;
  }
  
  .filter-tabs {
    display: flex;
    gap: 16rpx;
  }
  
  .tab-item {
    flex: 1;
    height: 64rpx;
    line-height: 64rpx;
    text-align: center;
    font-size: 26rpx;
    color: #666;
    background: #F5F7FA;
    border-radius: 8rpx;
    transition: all 0.3s;
  }
  
  .tab-item.active {
    color: #FFFFFF;
    background: #1890FF;
    box-shadow: 0 2rpx 8rpx rgba(24, 144, 255, 0.2);
  }
  
  .list-container {
    background: #FFFFFF;
    border-radius: 16rpx;
    padding: 24rpx;
  }
  
  .empty-state {
    width: 100%;
    text-align: center;
    color: #999;
    font-size: 28rpx;
    padding: 40rpx 0;
  }
  
  .empty-icon {
    width: 160rpx;
    height: 160rpx;
    margin-bottom: 20rpx;
  }

  /* 顶部标题栏 */
  .header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 24rpx 32rpx;
    background: #fff;
    border-bottom: 1rpx solid #eee;
  }

  .page-title {
    font-size: 36rpx;
    font-weight: 500;
    color: #333;
  }

  .logout-btn {
    font-size: 28rpx;
    color: #ff4d4f;
  }

  /* 数据统计 */
  .stats-row {
    display: flex;
    width: 100%;
    padding: 20rpx 0;
    margin-bottom: 20rpx;
  }

  .stat-item:not(:last-child)::after {
    content: '';
    position: absolute;
    right: 0;
    top: 10%;
    height: 80%;
    width: 1px;
    background: #eee;
  }

  .stat-value {
    display: block;
    font-size: 48rpx;
    color: #1890ff;
    font-weight: bold;
    line-height: 1;
    margin-bottom: 8rpx;
    text-align: center;
  }

  .stat-label {
    font-size: 28rpx;
    color: #666;
    text-align: center;
  }

  /* 状态筛选 */
  .filter-row {
    display: flex;
    width: 100%;
    padding: 0 20rpx;
    margin-bottom: 20rpx;
  }

  .filter-btn {
    flex: 1;
    height: 70rpx;
    line-height: 70rpx;
    text-align: center;
    font-size: 28rpx;
    color: #666;
    background: #f5f7fa;
    margin: 0 10rpx;
  }

  .filter-btn.active {
    color: #fff;
    background: #1890ff;
  }

  /* 列表区域 */
  .list-section {
    margin-top: 20rpx;
    padding: 0 32rpx;
  }

  /* 预约列表样式 */
  .reservation-list {
    padding: 0 20rpx;
  }
  
  .reservation-item {
    background: #fff;
    border-radius: 12rpx;
    padding: 20rpx;
    margin-bottom: 20rpx;
  }
  
  .item-header {
    display: flex;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 16rpx;
    padding-bottom: 16rpx;
    border-bottom: 1rpx solid #eee;
  }
  
  .item-time {
    font-size: 28rpx;
    color: #333;
  }
  
  .item-status {
    font-size: 24rpx;
    color: #1890ff;
  }
  
  .info-row {
    display: flex;
    margin-bottom: 12rpx;
  }
  
  .info-label {
    width: 120rpx;
    color: #666;
    font-size: 26rpx;
  }
  
  .info-value {
    flex: 1;
    color: #333;
    font-size: 26rpx;
  }

  .header-btns {
    display: flex;
    justify-content: space-between;
    padding: 30rpx 30rpx 0;
  }

  .clear-btn {
    color: #ff4d4f;
    font-size: 28rpx;
  }